    <Routine name="trial">
      <CodeComponent name="do_not_edit">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="# ** THIS CODE COMPONENT SHOULD BE AT THE TOP IN THE BUILDER VIEW **&amp;#10;# All code in this code component should be fully generic to any &amp;#10;# Bayesian Adaptive Design experiment and so should not be edited&amp;#10;# unless you know what you are doing.&amp;#10;&amp;#10;&amp;#10;# ONLY FOR BEN WHILE TESTING ------------------------------&amp;#10;import sys&amp;#10;sys.path.insert(0, '/Users/benjamv/git-local/badapted')&amp;#10;sys.path.insert(0, '/Users/benjamv/git-local/darc_toolbox')&amp;#10;# ---------------------------------------------------------&amp;#10;&amp;#10;&amp;#10;from badapted.designs import BayesianAdaptiveDesignGenerator&amp;#10;from darc_toolbox.utils import was_last_response_B&amp;#10;&amp;#10;&amp;#10;# Set up a logger for BAD related debugging&amp;#10;import logging&amp;#10;logging.basicConfig(filename=filename+'_BAD.log',&amp;#10;    level=logging.DEBUG,&amp;#10;    format='%(asctime)s:%(levelname)s:%(funcName)s:%(message)s')&amp;#10;&amp;#10;&amp;#10;# We will tell the PsychoPy trial loop thing that we will have an &amp;#10;# arbitrarily high number of trials. We specify number of trials through&amp;#10;# the Experiment Settings GUI. The trial routine is ended when our&amp;#10;# design generator returns None&amp;#10;max_trials = 666&amp;#10;&amp;#10;&amp;#10;# we start with no previous response&amp;#10;last_response_chose_B = None" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="import * as sys from 'sys';&amp;#10;import {BayesianAdaptiveDesignGenerator} from 'badapted/designs';&amp;#10;import {was_last_response_B} from 'darc_toolbox/utils';&amp;#10;import * as logging from 'logging';&amp;#10;var last_response_chose_B, max_trials;&amp;#10;sys.path.insert(0, &quot;/Users/benjamv/git-local/badapted&quot;);&amp;#10;sys.path.insert(0, &quot;/Users/benjamv/git-local/darc_toolbox&quot;);&amp;#10;logging.basicConfig({&quot;filename&quot;: (filename + &quot;_BAD.log&quot;), &quot;level&quot;: logging.DEBUG, &quot;format&quot;: &quot;%(asctime)s:%(levelname)s:%(funcName)s:%(message)s&quot;});&amp;#10;max_trials = 666;&amp;#10;last_response_chose_B = null;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="design = design_thing.get_next_design(model);&amp;#10;if ((design === null)) {&amp;#10;    console.log(&quot;Breaking out of trial loop now&quot;);&amp;#10;    break;&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# DESIGN GENERATOR =================================================&amp;#10;design = design_thing.get_next_design(model)&amp;#10;&amp;#10;# If we are out of designs, then break out of the trial loop&amp;#10;if design is None:&amp;#10;    print('Breaking out of trial loop now')&amp;#10;    break&amp;#10;&amp;#10;# NOTE TO EXPERIMENTER: if we want to do any adjustments to the design, &amp;#10;# such as rounding up significant figures etc, we should do that here to&amp;#10;# the actual design itself rather than just the text displayed to &amp;#10;# participants. That way the data we output will correspond exactly to the&amp;#10;# prospects actually shown to participants." valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="# export parameter summary&amp;#10;model.θ.describe().to_csv(filename+'_parameter_summary.csv')&amp;#10;&amp;#10;# export all particles, full posterior&amp;#10;model.θ.to_csv(filename+'_parameter_all_particles.csv')&amp;#10;&amp;#10;# visualise posterior distributions&amp;#10;try:&amp;#10;    model.posterior_triplot(filename)&amp;#10;except Exception as err:&amp;#10;    print('FAILED to export posterior histograms as .pdf')&amp;#10;    print(f'Error {err}')&amp;#10;" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="/* Syntax Error: Fix Python code */" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="psychoJS.window.flip();&amp;#10;last_response_chose_B = was_last_response_B(response.keys);&amp;#10;design_thing.enter_trial_design_and_response(design, last_response_chose_B);&amp;#10;model.update_beliefs(design_thing.data);&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# Clear screen as soon as we press the button. Useful because the code &amp;#10;# below can take a moment to run before things are cleared, which leads to &amp;#10;# crappy responsivity&amp;#10;win.flip()&amp;#10;&amp;#10;# Turn the response from PsychoPy into a True/False, did they choose B?&amp;#10;# `response` is the name of a PsychoPy keyboard component.&amp;#10;last_response_chose_B = was_last_response_B(response.keys)&amp;#10;&amp;#10;design_thing.enter_trial_design_and_response(design, last_response_chose_B)&amp;#10;&amp;#10;model.update_beliefs(design_thing.data)&amp;#10;" valType="extendedCode"/>
