        <Param name="Begin Experiment" updates="constant" val="# Import our DARC design code&amp;#10;from darc_toolbox import Design&amp;#10;from darc_toolbox.utils import days_to_string&amp;#10;from darc_toolbox.data_plotting import data_plotter&amp;#10;from darc_toolbox.delayed import models&amp;#10;from darc_toolbox.designs import DesignSpaceBuilder&amp;#10;&amp;#10;&amp;#10;# Process experimenter inputs from experiment settings GUI -------------&amp;#10;# Number of trials obtained from the PsychoPy GUI is stored as a string&amp;#10;# so we need to convert that to an integer.&amp;#10;expInfo['trials'] = int(expInfo['trials'])&amp;#10;# Convert number of particles from the GUI to an integer&amp;#10;expInfo['particles'] = int(expInfo['particles'])&amp;#10;&amp;#10;&amp;#10;# Now we will chose our (design, model) combination.&amp;#10;model = models.Hyperbolic(n_particles=expInfo['particles'])&amp;#10;D = DesignSpaceBuilder.delayed().build()&amp;#10;design_thing = BayesianAdaptiveDesignGenerator(D, max_trials=expInfo['trials'])&amp;#10;&amp;#10;&amp;#10;# If you want to customise the location and filename of saved data, then&amp;#10;# edit that in Experiment Settings &gt; Data tab.&amp;#10;# OR you can overwrite that with the following line, adapted for your &amp;#10;# goals&amp;#10;#     filename = _thisDir + os.sep + u'data/%s_%s_%s' % (expInfo['participant'], expName, expInfo['date'])&amp;#10;&amp;#10;&amp;#10;def design_to_display_strings(design):&amp;#10;    '''A function to convert a design into strings available for PsychoPy &amp;#10;    to print to screen.&amp;#10;    Experimenters can edit this code to customise how the stimuli are &amp;#10;    presented to participants. For example, you can change from £ to&amp;#10;    whatever your local currency symbol is.'''&amp;#10;    # rewards -------------------------&amp;#10;    text_RA = f'${design.RA:0.2f}'&amp;#10;    text_RB = f'${design.RB:0.2f}'&amp;#10;&amp;#10;    # delays --------------------------&amp;#10;    text_DA = days_to_string(design.DA)&amp;#10;    text_DB = days_to_string(design.DB)&amp;#10;&amp;#10;    # probabilities -------------------&amp;#10;    text_PA = f'{design.PA*100:3.0f}%'&amp;#10;    text_PB = f'{design.PB*100:3.0f}%'&amp;#10;    return (text_RA, text_RB, text_DA, text_DB, text_PA, text_PB)" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="&amp;#10;        // add-on: pad(n: number, width: number): string&amp;#10;        function pad(n, width) {&amp;#10;            width = width || 2;&amp;#10;            integerPart = Number.parseInt(n);&amp;#10;            decimalPart = (n+'').match(/\.[0-9]*/);&amp;#10;            if (!decimalPart)&amp;#10;                decimalPart = '';&amp;#10;            return (integerPart+'').padStart(width,'0') + decimalPart;&amp;#10;        }&amp;#10;&amp;#10;        import {Design} from 'darc_toolbox';&amp;#10;import {days_to_string} from 'darc_toolbox/utils';&amp;#10;import {data_plotter} from 'darc_toolbox/data_plotting';&amp;#10;import {models} from 'darc_toolbox/delayed';&amp;#10;import {DesignSpaceBuilder} from 'darc_toolbox/designs';&amp;#10;var D, design_thing, model;&amp;#10;expInfo[&quot;trials&quot;] = Number.parseInt(expInfo[&quot;trials&quot;]);&amp;#10;expInfo[&quot;particles&quot;] = Number.parseInt(expInfo[&quot;particles&quot;]);&amp;#10;model = new models.Hyperbolic({&quot;n_particles&quot;: expInfo[&quot;particles&quot;]});&amp;#10;D = DesignSpaceBuilder.delayed().build();&amp;#10;design_thing = new BayesianAdaptiveDesignGenerator(D, {&quot;max_trials&quot;: expInfo[&quot;trials&quot;]});&amp;#10;function design_to_display_strings(design) {&amp;#10;    /*A function to convert a design into strings available for PsychoPy&amp;#10;    to print to screen.&amp;#10;    Experimenters can edit this code to customise how the stimuli are&amp;#10;    presented to participants. For example, you can change from £ to&amp;#10;    whatever your local currency symbol is.*/&amp;#10;    var text_DA, text_DB, text_PA, text_PB, text_RA, text_RB;&amp;#10;    text_RA = `$${pad(Number.parseFloat(design.RA).toPrecision(2), 0)}`;&amp;#10;    text_RB = `$${pad(Number.parseFloat(design.RB).toPrecision(2), 0)}`;&amp;#10;    text_DA = days_to_string(design.DA);&amp;#10;    text_DB = days_to_string(design.DB);&amp;#10;    text_PA = `${pad(Number.parseFloat((design.PA * 100)).toPrecision(0), 3)}%`;&amp;#10;    text_PB = `${pad(Number.parseFloat((design.PB * 100)).toPrecision(0), 3)}%`;&amp;#10;    return [text_RA, text_RB, text_DA, text_DB, text_PA, text_PB];&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="[text_RA, text_RB, text_DA, text_DB, text_PA, text_PB] = design_to_display_strings(design);&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# convert the design into meaningful strings &amp;#10;text_RA, text_RB, text_DA, text_DB, text_PA, text_PB = design_to_display_strings(design)" valType="extendedCode"/>
